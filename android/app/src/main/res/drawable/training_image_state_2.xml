<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <padding android:left="6dp"
                android:top="6dp"
                android:bottom="6dp"
                android:right="6dp">
            </padding>
            <stroke android:color="@color/teal_200"
                android:width="4dp">
            </stroke>
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <stroke android:color="@color/main_color"
                android:width="2dp">
            </stroke>
        </shape>
    </item>
    <item>
        <bitmap android:src="@mipmap/ic_runner_man_foreground"
            android:tint="@color/teal_200"></bitmap>
    </item>
</layer-list>